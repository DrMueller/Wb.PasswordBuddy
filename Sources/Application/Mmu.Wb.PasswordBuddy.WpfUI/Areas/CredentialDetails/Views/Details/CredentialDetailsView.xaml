<UserControl x:Class="Mmu.Wb.PasswordBuddy.WpfUI.Areas.CredentialDetails.Views.Details.CredentialDetailsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Mmu.Wb.PasswordBuddy.WpfUI.Areas.CredentialDetails.Views"
             xmlns:credentialData="clr-namespace:Mmu.Wb.PasswordBuddy.WpfUI.Areas.CredentialDetails.Views.CredentialData"
             xmlns:changeHistory="clr-namespace:Mmu.Wb.PasswordBuddy.WpfUI.Areas.CredentialDetails.Views.ChangeHistory"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <credentialData:CredentialDataView />
        <changeHistory:ChangeHistoryView Grid.Row="1" />
    </Grid>
</UserControl>
